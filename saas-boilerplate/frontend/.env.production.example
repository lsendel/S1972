# Production Frontend Environment Variables Template
# Copy this to .env.production and fill in with your production values

# API Configuration
VITE_API_URL=https://api.yourdomain.com

# Stripe (Live Mode)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# OAuth Providers (Optional)
VITE_GOOGLE_OAUTH_CLIENT_ID=123456789-xxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com
VITE_GITHUB_OAUTH_CLIENT_ID=Iv1.xxxxxxxxxxxxxxxx

# Monitoring & Error Tracking
VITE_SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@o123456.ingest.sentry.io/123456
VITE_ENVIRONMENT=production

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_PANEL=false

# App Configuration
VITE_APP_NAME=Your SaaS App
VITE_APP_URL=https://app.yourdomain.com
