# E2E Testing Environment Variables
# These are used by Playwright tests for authentication and API calls

# Backend API URL
VITE_API_URL=http://localhost:8000/api/v1

# Test User Credentials (created by: python manage.py create_test_user)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=testpassword123

# Test Organization
TEST_ORG_NAME=Test Organization
TEST_ORG_SLUG=test-org
VITE_DEFAULT_ORG_SLUG=test-org

# Playwright Configuration
# These can be used in tests via process.env
HEADLESS=true
SLOW_MO=0
